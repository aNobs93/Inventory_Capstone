<!DOCTYPE HTML>
<html>
<head>
    <script type="text/javascript">
                    window.onload = function () {
                        var chart = new CanvasJS.Chart("chartContainer",
                            {
                                animationEnabled: true, 
		                        animationDuration: 2000,
                                backgroundColor: "#92d0de",
                                title:{
                                    text: @Html.Raw(ViewBag.Title)
                                },
                                data: [
                                    {
                                        type: "line",
                                        dataPoints: @Html.Raw(ViewBag.DataPoints)
                                    }
                                ]
                            });

                        chart.render();
                    }
    </script>
    <script type="text/javascript" src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
</head>
<body>
    <div id="chartContainer" style="height: 300px; width: 100%;">
    </div>
</body>
</html>

    @if (ViewBag.Key == false)
    {
    <a href="@Url.Action("Index", "Inventory")" class="btn btn-info btn-lg btn-block">Back</a>
    }
    @if (ViewBag.Key == true)
    {
    <a href="@Url.Action("Details", "InventoryProduct")" class="btn btn-info btn-lg btn-block">Back</a>
    }


